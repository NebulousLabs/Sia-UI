{
  "name": "Sia-UI",
  "version": "v1.2.2",
  "releaseName": "Blue Moon",
  "main": "dist/main.js",
  "author": "NebulousLabs",
  "description": "A UI application for interfacing with Sia",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.0",
